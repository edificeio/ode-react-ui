{
  "name": "@ode-react-ui/core",
  "version": "1.0.1",
  "private": false,
  "description": "@ode-react-ui react core components",
  "keywords": [
    "react",
    "frontend",
    "components",
    "ui"
  ],
  "homepage": "https://github.com/opendigitaleducation/ode-react-ui/tree/main/libs/core#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/ode-react-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/ode-react-ui.git"
  },
  "license": "AGPL-3.0",
  "author": "Open Digital Education",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build --config ../../config/vite/vite.config.ts",
    "fix": "eslint --fix .",
    "format": "concurrently \"yarn:format:*\"",
    "format:check": "yarn prettier --check .",
    "format:write": "yarn prettier --write .",
    "lint": "eslint ."
  },
  "prettier": "@ode-react-ui/prettier-config-custom",
  "dependencies": {
    "@ode-react-ui/hooks": "*",
    "@ode-react-ui/icons": "*",
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ode-react-ui/eslint-config-custom": "*",
    "@ode-react-ui/prettier-config-custom": "*",
    "@ode-react-ui/typescript-config-custom": "*",
    "@storybook/react": "6.5.12",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.4.0"
  },
  "peerDependencies": {
    "@ode-react-ui/hooks": "*",
    "@ode-react-ui/icons": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
