{
  "name": "@ode-react-ui/hooks",
  "version": "0.0.1",
  "private": false,
  "description": "@ode-react-ui react hooks library",
  "keywords": [
    "react",
    "frontend",
    "hooks",
    "ui"
  ],
  "homepage": "https://github.com/opendigitaleducation/ode-react-ui/tree/main/libs/hooks#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/ode-react-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/ode-react-ui.git"
  },
  "license": "GPL-3.0",
  "author": "Open Digital Education",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "yarn prebuild && tsc --noEmit && vite build --config ../../config/vite/vite.config.ts",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "format": "concurrently \"yarn:format:*\"",
    "format:check": "yarn prettier --check .",
    "format:write": "yarn prettier --write ."
  },
  "prettier": "@ode-react-ui/prettier-config-custom",
  "dependencies": {
    "clsx": "1.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ode-react-ui/eslint-config-custom": "*",
    "@ode-react-ui/prettier-config-custom": "*",
    "@ode-react-ui/typescript-config-custom": "*",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "concurrently": "7.4.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
