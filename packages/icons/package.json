{
  "name": "@ode-react-ui/icons",
  "version": "0.0.1",
  "description": "@ode-react-ui react icons library",
  "keywords": [
    "react",
    "frontend",
    "icons",
    "ui"
  ],
  "homepage": "https://github.com/opendigitaleducation/ode-react-ui/tree/main/packages/icons#readme",
  "bugs": {
    "url": "https://github.com/opendigitaleducation/ode-react-ui/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opendigitaleducation/ode-react-ui.git"
  },
  "license": "GPL-3.0",
  "author": "Open Digital Education",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn svgr && vite build --config ../../config/vite/vite.config.ts",
    "dev": "vite",
    "lint": "eslint -c ../../config/eslint/icons.json .",
    "lint:fix": "eslint --fix .",
    "prettier": "concurrently \"yarn:prettier:*\"",
    "prettier:check": "npx prettier --config ../../config/prettier.config.js --check .",
    "prettier:write": "npx prettier --config ../../config/prettier.config.js --write .",
    "preview": "vite preview",
    "svgr": "npx @svgr/cli --icon --replace-attr-values '#4A4A4A=currentColor' --jsx-runtime automatic --title-prop --typescript --out-dir src assets"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.16",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "concurrently": "7.4.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  }
}
